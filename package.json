{
  "name": "fuers-allgaeu",
  "version": "0.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcradziwill/fuers-allgaeu.git"
  },
  "license": "MIT",
  "author": "Marc Radziwill <hallo@marcradziwill.com>",
  "scripts": {
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "build-and-test": "npm run build && npm run test:e2e",
    "copyapi": "rsync -av --progress api public --exclude api/node_modules --exclude now.json",
    "cy": "is-ci \"test:e2e\" \"cy:open\"",
    "cy:open": "cypress open",
    "cy:run": "CYPRESS_BASE_URL=http://localhost:5020 cypress run",
    "dev": "npm-run-all --parallel dev:*",
    "dev:functions": "cd api && now dev",
    "dev:gatsby": "gatsby develop",
    "develop": "npm run start",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "install:cypress": "cypress install",
    "lint": "eslint .",
    "lint:fix": "npm run lint --fix",
    "now-build": "gatsby build",
    "now:cypress": "npm-run-all --parallel install:cypress build && npm run test:e2e",
    "serve": "gatsby serve",
    "setup": "npm i && npm validate",
    "start": "gatsby serve --port 5020",
    "test:deploy": "jest --silent",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:e2e": "start-server-and-test start http://localhost:5020 cy:run",
    "test:lighthouse": "lighthouse http://localhost:5020 --budget-path=./tests/budget.json",
    "test:watch": "jest --watch",
    "validate": "npm-run-all --parallel lint test:coverage build-and-test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "ie >= 9"
  ],
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@loadable/component": "5.12.0",
    "@mdx-js/mdx": "1.5.7",
    "@mdx-js/react": "1.5.7",
    "@sindresorhus/slugify": "0.11.0",
    "dotenv": "8.2.0",
    "gatsby": "2.19.43",
    "gatsby-image": "2.2.43",
    "gatsby-link": "2.2.30",
    "gatsby-plugin-catch-links": "2.1.27",
    "gatsby-plugin-emotion": "4.1.24",
    "gatsby-plugin-google-tagmanager": "2.1.26",
    "gatsby-plugin-manifest": "2.2.47",
    "gatsby-plugin-offline": "3.0.40",
    "gatsby-plugin-react-helmet": "3.1.23",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sass": "2.1.30",
    "gatsby-plugin-sharp": "2.5.1",
    "gatsby-plugin-sitemap": "2.2.29",
    "gatsby-plugin-typography": "2.3.24",
    "gatsby-source-drive": "0.0.2",
    "gatsby-source-filesystem": "2.1.54",
    "gatsby-source-graphql": "2.1.34",
    "gatsby-transformer-csv": "2.2.1",
    "gatsby-transformer-sharp": "2.4.1",
    "graphql": "14.6.0",
    "lodash": "4.17.15",
    "node-sass": "4.13.1",
    "react": "16.13.0",
    "react-adaptive-hooks": "0.0.8",
    "react-dom": "16.13.0",
    "react-helmet": "~5.2.1",
    "react-markdown": "4.3.1",
    "react-share": "4.1.0",
    "react-typography": "0.16.19",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "16.12.0",
    "@now/node": "1.4.1",
    "@testing-library/cypress": "6.0.0",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.0",
    "babel-jest": "25.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-gatsby": "0.2.35",
    "babel-runtime": "6.26.0",
    "cypress": "4.1.0",
    "dotenv-cli": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-kentcdodds": "14.8.0",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-jest": "23.8.2",
    "gatsby-transformer-json": "2.2.27",
    "googleapis": "48.0.0",
    "husky": "4.2.3",
    "identity-obj-proxy": "3.0.0",
    "is-ci-cli": "2.0.0",
    "jest": "25.1.0",
    "jest-in-case": "1.0.2",
    "lint-staged": "10.0.8",
    "ncp": "2.0.0",
    "nodemailer": "6.4.5",
    "now": "17.0.4",
    "npm-run-all": "4.1.5",
    "ow": "0.17.0",
    "prettier": "1.18.2",
    "rsync": "0.6.1",
    "start-server-and-test": "1.10.10",
    "unist-util-visit": "2.0.2",
    "webpack-bundle-analyzer": "3.6.1"
  }
}
